{
  "manifest_version": 3,
  "name": "Googs",
  "version": "1.0.0",
  "icons": {
    "32": "images/github-icon.png"
  },
  "permissions": [
    "tabGroups",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "https://*/*",
        "https://*/*"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
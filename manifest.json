{
  "manifest_version": 3,
  "name": "Googs",
  "version": "1.0.0",
  "icons": {
    "32": "images/googs32.png"
  },
  "permissions": [
    "tabGroups",
    "activeTab",
    "scripting",
    "tabs",
    "storage"
  ],
  "action": {
    "default_popup": "test/index.html"
  },
  "content_scripts": [
    {
      "js": ["notes/notes.js"],
      "css": ["notes/notes.css"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}